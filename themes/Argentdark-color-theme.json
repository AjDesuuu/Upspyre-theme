{
    "name": "Argentdark",
    "type": "dark",
    "colors": {
        "editor.background": "#1e1e1e",
        "editor.foreground": "#d4d4d4",
        "editorCursor.foreground": "#ffffff",
        "editor.selectionBackground": "#264f78",
        "editor.lineHighlightBackground": "#2d2d2d",
        "editorWhitespace.foreground": "#404040"
    },
    "tokenColors": [
        {
            "scope": "keyword.control.upspyre",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "string.quoted.double.upspyre",
            "settings": {
                "foreground": "#ce9178"
            }
        },
        {
            "scope": "constant.numeric.upspyre",
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "scope": "comment.line.double-slash.upspyre",
            "settings": {
                "foreground": "#6a9955"
            }
        },
        {
            "scope": "punctuation.definition.upspyre",
            "settings": {
                "foreground": "#d4d4d4"
            }
        }
    ]
}